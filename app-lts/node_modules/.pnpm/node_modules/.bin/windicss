#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/cli/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/cli/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules/windicss/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/windicss@3.5.6/node_modules:/Users/chrisbrown/Desktop/mysten-hackathon/app-lts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../windicss/cli/index.js" "$@"
else
  exec node  "$basedir/../windicss/cli/index.js" "$@"
fi
